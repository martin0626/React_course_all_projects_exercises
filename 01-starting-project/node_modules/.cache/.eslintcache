 state;\n};\n\nconst passwordReducer = (state, action) => {\n  if (action.type === \"USER_INPUT\") {\n    return {\n      value: action.val,\n      isValid: action.val.trim().length > 6,\n    };\n  }\n  if (action.type === \"INPUT_BLUR\") {\n    return {\n      value: state.value,\n      isValid: state.value.trim().length > 6,\n    };\n  }\n  return state;\n};\n\nconst Login = (props) => {\n  const [formIsValid, setFormIsValid] = useState(false);\n\n  const [emailState, dispatchEmail] = useReducer(emailReducer, {\n    value: \"\",\n    isValid: true,\n  });\n\n  const [passwordState, dispatchPassword] = useReducer(passwordReducer, {\n    value: \"\",\n    isValid: true,\n  });\n\n  let { isValid: emailIsValid, value: email } = emailState;\n  let { isValid: passwordIsValid, value: password } = passwordState;\n\n  useEffect(() => {\n    setFormIsValid(\n      emailIsValid &&\n        passwordIsValid &&\n        password.trim().length > 0 &&\n        email.trim().length > 0\n    );\n  }, [emailIsValid, passwordIsValid]);\n\n  const emailChangeHandler = (event) => {\n    dispatchEmail({ type: \"USER_INPUT\", val: event.target.value });\n\n    setFormIsValid(emailState.isValid && passwordState.isValid);\n  };\n\n  const passwordChangeHandler = (event) => {\n    dispatchPassword({ type: \"USER_INPUT\", val: event.target.value });\n\n    setFormIsValid(\n      event.target.value.trim().length > 6 && emailState.value.includes(\"@\")\n    );\n  };\n\n  const validateEmailHandler = () => {\n    dispatchEmail({ type: \"INPUT_BLUR\", val: emailState.value });\n  };\n\n  const validatePasswordHandler = () => {\n    dispatchPassword({ type: \"INPUT_BLUR\", val: passwordState.value });\n  };\n\n  const submitHandler = (event) => {\n    event.preventDefault();\n    props.onLogin(emailState.value, passwordState.value);\n  };\n\n  useEffect(() => {\n    console.log(passwordState.isValid, passwordState.value);\n  }, [passwordState]);\n\n  return (\n    <Card className={classes.login}>\n      <form onSubmit={submitHandler}>\n        <div\n          className={`${classes.control} ${\n            emailState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"email\">E-Mail</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            value={emailState.value}\n            onChange={emailChangeHandler}\n            onBlur={validateEmailHandler}\n          />\n        </div>\n        <div\n          className={`${classes.control} ${\n            passwordState.isValid === false ? classes.invalid : \"\"\n          }`}\n        >\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            value={passwordState.value}\n            onChange={passwordChangeHandler}\n            onBlur={validatePasswordHandler}\n          />\n        </div>\n        <div className={classes.actions}>\n          <Button type=\"submit\" className={classes.btn} disabled={!formIsValid}>\n            Login\n          </Button>\n        </div>\n      </form>\n    </Card>\n  );\n};\n\nexport default Login;\n","D:\\React_Course\\ReactProjects\\React_course_all_projects_exercises\\01-starting-project\\src\\components\\MainHeader\\Navigation.js",[],[],"D:\\React_Course\\ReactProjects\\React_course_all_projects_exercises\\01-starting-project\\src\\components\\UI\\Card\\Card.js",[],[],"D:\\React_Course\\ReactProjects\\React_course_all_projects_exercises\\01-starting-project\\src\\components\\UI\\Button\\Button.js",[],[],{"ruleId":"44","severity":1,"message":"45","line":56,"column":6,"nodeType":"46","endLine":56,"endColumn":37,"suggestions":"47"},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'email' and 'password'. Either include them or remove the dependency array.","ArrayExpression",["48"],{"desc":"49","fix":"50"},"Update the dependencies array to be: [email, emailIsValid, password, passwordIsVal